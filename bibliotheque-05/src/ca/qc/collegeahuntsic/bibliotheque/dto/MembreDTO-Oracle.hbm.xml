<?xml version="1.0"
      encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!--
   - PretDTO mapping file.
   -
   - Author: Gilles BÃ©nichou
-->

<hibernate-mapping>
  <class name="ca.qc.collegeahuntsic.bibliotheque.dto.MembreDTO"
         table="Membre">
         <id name="idMembre"
             type="string">
             <column name="IDMEMBRE">
             </column>
             <generator class="sequence">
                        <param name="sequence">SEQ_ID_MEMBRE</param>
             </generator>
         </id>

                 <property name="nom"
                   type="string">
                   <column name="NOM">
                   </column>
         </property>
         <property name="telephone"
                   type="long">
                   <column name="TELEPHONE">
                   </column>
         </property>
         <property name="limitePret"
                  type="int">
                  <column name="LIMITEPRET">
                  </column>
         </property>

         <many-to-one name="membreDTO"
                      class="ca.qc.collegeahuntsic.bibliotheque.dto.MembreDTO"
                      not-null="true">
                      <column name="IDMEMBRE">
                      </column>
         </many-to-one>
         <many-to-one name="livreDTO"
                      class="ca.qc.collegeahuntsic.bibliotheque.dto.LivreDTO"
                      not-null="true">
                      <column name="IDLIVRE">
                      </column>
         </many-to-one>
  </class>
</hibernate-mapping>
